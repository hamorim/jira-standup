{
    "manifest_version": 2,
 
    "name": "Jira Standup", 
    "description": "Better Jira Standup View.",
    "version": "1.0",
    "author": "Hugo Lyra",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Jira Standup"
    },
    "permissions": [
        "activeTab" 
    ],
    "web_accessible_resources": [
        "styles.css"
    ]
}